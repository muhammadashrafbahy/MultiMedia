      <Button fx:id="HuffmanCode" layoutX="155.0" layoutY="100.0" mnemonicParsing="false" onAction="#HuffmanCode" text="HuffmanCode" />
      <Button fx:id="lzw" layoutX="292.0" layoutY="100.0" mnemonicParsing="false" onAction="#lzw" text="LZW" />
      <Button fx:id="shannonfano" layoutX="375.0" layoutY="100.0" mnemonicParsing="false" onAction="#shannonfano" text="ShannonFano" />
      <Button fx:id="arth" layoutX="507.0" layoutY="100.0" mnemonicParsing="false" onAction="#arth" text="Arithmetic" />
      <Label fx:id="output" layoutX="220.0" layoutY="183.0" />
      <Button fx:id="runlen_decompress" layoutX="37.0" layoutY="333.0" mnemonicParsing="false" onAction="#runlen_decompress" text="run length Decompress" />
